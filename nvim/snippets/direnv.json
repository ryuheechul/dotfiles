{
  "minimal-nix": {
    "prefix": "nix",
    "body": [
      "# vim: ft=bash",
      "source_up",
      "use nix"
    ],
    "description": "configure nix-shell via direnv"
  },
  "source_env": {
    "prefix": ".env",
    "body": [
      "test -f .env && source_env .env"
    ],
    "description": "source .env if .env exist"
  }
}
