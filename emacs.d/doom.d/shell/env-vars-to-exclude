#!/usr/bin/env bash

# this will be used few places including ../init.el and ./source.zsh

# both for doom sync and on loading shell
echo -n "TERM
TERM_PROGRAM
TERM_PROGRAM_VERSION
TERM_SESSION_ID
ITERM_PROFILE
ITERM_SESSION_ID
LC_TERMINAL
LC_TERMINAL_VERSION
TMUX
FZF_TMUX
TMUX_PANE
TMUX_PLUGIN_MANAGER_PATH
tmux_version
ZELLIJ
ZELLIJ_SESSION_NAME
ZELLIJ_PANE_ID
ALACRITTY_SOCKET
