#!/usr/bin/env bash

# this will be used few places including ../init.el and ./source.zsh

# both for doom sync and on loading shell
echo -n "TERM
TERM_PROGRAM
TERM_PROGRAM_VERSION
TERM_SESSION_ID
ITERM_PROFILE
ITERM_SESSION_ID
LC_TERMINAL
LC_TERMINAL_VERSION
TMUX
FZF_TMUX
TMUX_PANE
TMUX_PLUGIN_MANAGER_PATH
tmux_version
ALACRITTY_SOCKET
ALACRITTY_LOG
"

# only for doom sync - things that I don't want shell to unset
test -n "${FOR_DOOM_SYNC_ENV}" && \
  echo -n "
DISPLAY
STARSHIP_SHELL
STARSHIP_SESSION_KEY
LANG
LS_COLORS
INSIDE_EMACS
INSIDE_DOOM_EMACS
EMACS_VTERM_PATH
DOOM_EMACS_THEME
PROOT
NVIM_LISTEN_ADDRESS
TERMINFO_DIRS"
