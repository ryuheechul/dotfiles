  echo -n "DISPLAY
XAUTHORITY
SSH_CLIENT
SSH_TTY
SSH_CONNECTION
STARSHIP_SHELL
STARSHIP_SESSION_KEY
LANG
LS_COLORS
INSIDE_EMACS
INSIDE_DOOM_EMACS
ADDITIONAL_PATH_FOR_EMACS
EMACS_VTERM_PATH
DOOM_EMACS_THEME
PROOT
NVIM_LISTEN_ADDRESS
SHLVL
TERMINFO_DIRS"
